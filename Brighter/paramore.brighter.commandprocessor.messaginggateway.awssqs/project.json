{
  "buildOptions": {
    "debugType": "portable"
  },
  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "Newtonsoft.Json": "9.0.1",
    "Microsoft.Bcl" : "1.1.10",
    "Microsoft.Bcl.Async" : "1.0.168",
    "Microsoft.Bcl.Build" : "1.0.21",
    "Microsoft.Net.Http" : "2.2.29",
    "PCLStorage" : "1.0.2",
    "PCLCrypto" : "2.0.147",
    "paramore.brighter.commandprocessor": {
      "target": "project"
    },
    "AWSSDK.Core": "3.3.4",
    "AWSSDK.SimpleNotificationService": "3.3.0.2",
    "AWSSDK.SQS": "3.3.0.2",
    "Validation": "2.3.7" },
  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "System.Threading.Thread": "4.0.0"
      }
    },
    "net451": {
      "dependencies": {
        "PInvoke.BCrypt": "0.3.152",
        "PInvoke.NCrypt": "0.3.152",
        "PInvoke.Windows.Core": "0.3.152"
      }
    },
    "net452": {
      "dependencies": {
        "PInvoke.BCrypt": "0.3.152",
        "PInvoke.NCrypt": "0.3.152",
        "PInvoke.Windows.Core": "0.3.152"
      }
    }
  }, 
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  }
}
