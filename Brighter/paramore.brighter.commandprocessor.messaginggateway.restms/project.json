{
  "buildOptions": {
    "debugType": "portable"
  },
  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "Newtonsoft.Json": "9.0.1",
    "paramore.brighter.commandprocessor": {
      "target": "project"
    },
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "System.IO": "4.1.0",
        "System.Private.DataContractSerialization": "4.1.1",
        "System.Runtime": "4.1.0",
        "System.Runtime.Serialization.Xml": "4.1.1",
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Text.Encoding": "4.0.11",
        "System.Threading.Thread": "4.0.0",
        "System.Xml.ReaderWriter": "4.0.11",
        "System.Xml.XmlSerializer": "4.0.11",
        "System.Net.Requests": "4.0.11",
        "Hawk": "2.0.0-beta5"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Net.Requests": "4.0.0",      
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Xml.XmlSerializer": "4.0.0",
        "System.Xml": "4.0.0"
      },
      "dependencies": {
        "System.Runtime.Serialization.Xml": "4.0.0",
        "System.Runtime.Serialization.Primitives": "4.0.0",
        "Thinktecture.IdentityModel.Hawk": "2.1.2"
      }
    },
    "net452": {
      "frameworkAssemblies": {
        "System.Net.Requests": "4.0.0",      
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Xml.XmlSerializer": "4.0.0",
        "System.Xml": "4.0.0"
      },
      "dependencies": {
        "System.Runtime.Serialization.Xml": "4.0.0",
        "System.Runtime.Serialization.Primitives": "4.0.0",
        "Thinktecture.IdentityModel.Hawk": "2.1.2"
      }
    }
  }, 
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  }
}
