{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable"
  },
   "dependencies": {
    "Microsoft.Data.Sqlite": "1.1.0",
    "NETStandard.Library": "1.6.1",
    "Newtonsoft.Json": "9.0.1",
    "paramore.brighter.commandprocessor": {
      "target": "project"
    }
  },
  "frameworks": {
    "netstandard1.5": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "System.Threading.Thread": "4.3.0"
      }
    },
   "net452": {}
  }, 
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  }
}
