<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Azure Service Bus Scheduler integration for Paramore.Brighter Command Processor. Provides message scheduling capabilities using Azure Service Bus scheduled messages for delayed message delivery and recurring tasks.</Description>
    <Authors>Rafael Andrade</Authors>
    <TargetFrameworks>$(BrighterTargetFrameworks)</TargetFrameworks>
    <PackageTags>Azure;Service Bus;Scheduler;Message Scheduling;Delayed Messages;Cloud;Command Processor;Brighter</PackageTags>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Paramore.Brighter.MessagingGateway.AzureServiceBus\Paramore.Brighter.MessagingGateway.AzureServiceBus.csproj"/>
    <ProjectReference Include="..\Paramore.Brighter\Paramore.Brighter.csproj"/>
  </ItemGroup>
</Project>
