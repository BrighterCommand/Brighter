<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>SQLite inbox provider for Paramore.Brighter Command Processor. Enables inbox pattern implementation for idempotent command and event handling using SQLite as the persistence layer.</Description>
    <Authors>Ian Cooper</Authors>
    <TargetFrameworks>$(BrighterTargetFrameworks)</TargetFrameworks>
    <PackageTags>SQLite;Database;Inbox;Idempotent;Command;Event;Command Processor;Embedded Database;Brighter</PackageTags>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.Sqlite" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Paramore.Brighter.Sqlite\Paramore.Brighter.Sqlite.csproj" />
    <ProjectReference Include="..\Paramore.Brighter\Paramore.Brighter.csproj" />
  </ItemGroup>
</Project>