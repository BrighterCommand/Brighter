<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Amazon DynamoDB v4 inbox provider for Paramore.Brighter Command Processor. Enables inbox pattern implementation for idempotent command and event handling using DynamoDB v4 SDK as the persistence layer.</Description>
    <TargetFrameworks>$(BrighterTargetFrameworks)</TargetFrameworks>
    <PackageTags>DynamoDB;AWS;NoSQL;Inbox;Idempotent;Command;Event;Command Processor;Database;SDK v4;Brighter</PackageTags>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.DynamoDBv2" VersionOverride="$(AwsDynamoDbV4)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Paramore.Brighter\Paramore.Brighter.csproj"/>
  </ItemGroup>
</Project>