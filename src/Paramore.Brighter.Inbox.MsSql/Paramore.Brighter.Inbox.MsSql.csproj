<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Microsoft SQL Server inbox provider for Paramore.Brighter Command Processor. Enables inbox pattern implementation for idempotent command and event handling using SQL Server as the persistence layer.</Description>
    <Authors>Francesco Pighi</Authors>
    <TargetFrameworks>$(BrighterFrameworkAndCoreTargetFrameworks)</TargetFrameworks>
    <PackageTags>SQL Server;MSSQL;Database;Inbox;Idempotent;Command;Event;Command Processor;Relational Database;Brighter</PackageTags>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="DDL Scripts\MSSQL\Inbox.sql"/>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Paramore.Brighter.MsSql\Paramore.Brighter.MsSql.csproj"/>
    <ProjectReference Include="..\Paramore.Brighter\Paramore.Brighter.csproj"/>
  </ItemGroup>
</Project>