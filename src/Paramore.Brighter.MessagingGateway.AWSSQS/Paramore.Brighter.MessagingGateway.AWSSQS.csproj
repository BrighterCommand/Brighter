<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Amazon SQS messaging gateway implementation for Paramore.Brighter Command Processor. Provides decoupled message transport using Amazon Simple Queue Service with support for SNS notifications and AWS security integration.</Description>
    <Authors>Deniz Kocak</Authors>
    <TargetFrameworks>$(BrighterTargetFrameworks)</TargetFrameworks>
    <PackageTags>AWS;SQS;SNS;Amazon;Message Gateway;Command;Event;Service Activator;Decoupled;Invocation;Messaging;Remote;Command Dispatcher;Command Processor;Request;Service;Task Queue;Work Queue;Retry;Circuit Breaker;Availability;Brighter</PackageTags>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Paramore.Brighter\Paramore.Brighter.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.SecurityToken" />
    <PackageReference Include="Polly.Contrib.WaitAndRetry" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" />
    <PackageReference Include="AWSSDK.SQS" />
    

  </ItemGroup>
</Project>
