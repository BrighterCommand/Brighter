<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
      <Description>Google Cloud Firestore inbox provider for Paramore.Brighter Command Processor. Enables inbox pattern implementation for idempotent command and event handling using Firestore as the NoSQL document persistence layer.</Description>
      <Authors>Rafael Andrade</Authors>
      <TargetFrameworks>$(BrighterTargetFrameworks)</TargetFrameworks>
      <PackageTags>Firestore;Google Cloud;NoSQL;Inbox;Idempotent;Command;Event;Command Processor;Document Database;GCP;Brighter</PackageTags>
      <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\Paramore.Brighter.Firestore\Paramore.Brighter.Firestore.csproj" />
      <ProjectReference Include="..\Paramore.Brighter\Paramore.Brighter.csproj" />
    </ItemGroup>

</Project>
