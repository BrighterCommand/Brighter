<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Microsoft SQL Server messaging gateway implementation for Paramore.Brighter Command Processor. Provides decoupled message transport using SQL Server as a message queue with database-backed reliability.</Description>
    <Authors>Fred Hoogduin</Authors>
    <TargetFrameworks>$(BrighterFrameworkAndCoreTargetFrameworks)</TargetFrameworks>
    <Nullable>enable</Nullable>
    <PackageTags>SQL Server;Message Gateway;Database;Command;Event;Service Activator;Decoupled;Invocation;Messaging;Remote;Command Dispatcher;Command Processor;Request;Service;Task Queue;Work Queue;Retry;Circuit Breaker;Availability;Brighter</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Paramore.Brighter.MsSql\Paramore.Brighter.MsSql.csproj" />
    <ProjectReference Include="..\Paramore.Brighter\Paramore.Brighter.csproj" />
  </ItemGroup>

</Project>